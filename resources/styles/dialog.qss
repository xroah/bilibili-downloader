.body {
    background-color: #fff;
}

PushButton {
    min-height: 20px;
    border-radius: 3px;
    background-color: #fff;
}

PushButton:hover {
    background-color: #ddd;
}

PushButton:focus {
    border: 3px solid #9bc2fd;
}

.primary {
    color: #fff;
    border-radius: 3px;
    background-color: rgba(13, 110, 253, .8);
}

.primary:hover {
    background-color: rgba(13, 110, 253, .9);
}

.primary:pressed {
    background-color: rgba(13, 110, 253, 1);
}

.footer {
    background-color: #f0f0f0;
}

.footer PushButton,
.footer QPushButton {
    width: 60px;
    height: 26px;
    padding: 5px;
    margin-right: 5px;
    margin-left: 10px;
}
